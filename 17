programa {

	funcao inicio() {

	    inteiro numeros[10]={12,20,30,40,123,146,345,654,12345,7654}

	    inteiro mn=0

		maior(numeros,mn)

		escreva(maior(numeros,mn))

	}

	

	funcao inteiro maior(inteiro numeros[],inteiro mn){

	    para(inteiro i=0;i<9;i++){

	        

	        se(numeros[i]>numeros[i+1]){

	            mn=numeros[i]

	        }

	   }

	   retorne mn

	}

}
